<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">

  <test name="FoxTest">  
  <parameter name="browsers" value="firefox"></parameter>
  <classes>  
  <class name = "Runners.autoRunnerFox"/>  
  <class name = "Pages.RunHomeAndLogin"/>     
  </classes>
  </test> <!-- Test -->
  

  <test name="ChromeTest">  
  <parameter name="browsers" value="chrome"></parameter>
  <classes>  
  <class name = "Runners.autoRunnerChrome"/>    
  <class name = "com.dtt.tests.dttRun"/>    
  </classes>
  </test> <!-- Test -->
  
   <test name="OperaTest">  
  <parameter name="browsers" value="Opera"></parameter>
  <classes> 
  <class name = "com.dtt.tests.dttPOMRunner"/>   
  <class name = "Runners.AddAddressRunner"/>  
  </classes>
  </test> <!-- Test -->
  
  <test name="IETest">  
  <parameter name="browsers" value="IE"></parameter>
  <classes>  
  <class name = "Runners.autoRunnerIE"/> 
  <class name = "Pages.RunHomeAndLogin"/>     
  </classes>
  </test> <!-- Test -->
  
  
</suite> <!-- Suite -->
