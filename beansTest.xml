<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="Factory" class="Utility.BrowserFactory"></bean>
	<bean id="ScreenPrint" class="Utility.ScreenPrint"></bean>
	<bean id="DriverInstance" class = "WebDriver.DriverInstance"></bean>
	<bean id="addressPage" class="Pages.AddressPage">
		<constructor-arg value="driver"></constructor-arg>
	</bean>
	<bean id="homePage" class="Pages.HomePage">
		<constructor-arg value="driver"></constructor-arg>
	</bean>
	<bean id="orderPage" class="Pages.OrderHistoryPage">
		<constructor-arg value="driver"></constructor-arg>
	</bean>
	<bean id="login" class="Pages.loginPage">
		<constructor-arg value="driver"></constructor-arg>
	</bean>
</beans>
